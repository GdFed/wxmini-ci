<view class="page">
  <view class="page-post">
    <image src="https://s1.huishoubao.com/static/m/common/apple100_bg1.png" class="bg" mode="widthFix" />
    <image src="https://s1.huishoubao.com/static/m/common/apple100_bg2.png" class="bg" mode="widthFix" />
    <image onTap="gotoEval" data-url="{{couponUrl}}" src="https://s1.huishoubao.com/static/m/common/apple100_bg3.png" class="bg" mode="widthFix" />
    <image src="https://s1.huishoubao.com/static/m/common/apple100_bg4.png" class="bg" mode="widthFix" />
    <image onTap="gotoEval" data-url="{{highEvalUrl}}" src="https://s1.huishoubao.com/static/m/common/apple100_bg5.gif" class="bg" mode="widthFix" />
    <view class="products">
      <image src="https://s1.huishoubao.com/static/m/common/apple100_bg6.png" class="bg" mode="widthFix" />
      <view class="inner">
        <view class="item" onTap="gotoEval" data-url="{{item.url}}" a:for="{{products}}"></view>
      </view>
    </view>
    <image src="https://s1.huishoubao.com/static/m/common/apple100_bg7.png" class="bg" mode="widthFix" />
    <image onTap="gotoIndex" src="https://s1.huishoubao.com/static/m/common/apple100_bg8.png" class="bg" mode="widthFix" />
  </view>
</view>