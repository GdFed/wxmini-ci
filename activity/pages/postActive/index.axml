<view class="page">
  <view class="page-post">
    <image src="https://s1.huishoubao.com/static/m/common/post-active-bg1.png" class="bg" mode="widthFix" />
    <image onTap="gotoEval" data-url="{{limitTimeSale}}" src="https://s1.huishoubao.com/static/m/common/post-active-bg2.png" class="bg" mode="widthFix" />
    <image onTap="gotoEval" data-url="{{onlyProducts[0].url}}" src="https://s1.huishoubao.com/static/m/common/post-active-bg3.png" class="bg" mode="widthFix" />
    <image onTap="gotoEval" data-url="{{onlyProducts[1].url}}" src="https://s1.huishoubao.com/static/m/common/post-active-bg4.png" class="bg" mode="widthFix" />
    <image onTap="gotoEval" data-url="{{onlyProducts[2].url}}" src="https://s1.huishoubao.com/static/m/common/post-active-bg5.png" class="bg" mode="widthFix" />
    <view class="products">
      <image src="https://s1.huishoubao.com/static/m/common/post-active-bg6.png" class="bg" mode="widthFix" />
      <view class="inner">
        <view class="item" onTap="gotoEval" data-url="{{item.url}}" a:for="{{products}}"></view>
      </view>
    </view>
    <view class="brands">
      <image src="https://s1.huishoubao.com/static/m/common/post-active-bg7.png" class="bg" mode="widthFix" />
      <view class="inner">
        <view class="item" onTap="gotoEval" data-url="{{item.url}}" a:for="{{brands}}"></view>
      </view>
    </view>
    <image src="https://s1.huishoubao.com/static/m/common/post-active-bg8.png" class="bg" mode="widthFix" />
  </view>
</view>